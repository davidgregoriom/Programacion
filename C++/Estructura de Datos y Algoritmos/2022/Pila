struct ListNode{
    int element;
    ListNode* next;
};

class StackInt{
public:
    StackInt();
    ListNode *topofStack;

private:

};


int main()
{
  StackInt pila = StackInt();
  pila.push(7);
  cout << pila.top() << endl;
}

